{
  "version": "1.0.0",
  "configurations": [
    {
      "name": "Launch Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}" ],
      "stopOnEntry": true,
      "sourceMaps": true,
      "preLaunchTask": "npm",
      "presentation": {
        "hidden": false,
        "group": "",
        "order": 1
      }
    }
  ]
}
