{
  "version": "1.0.0",
  "configurations": [
    {
      "name": "Launch Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}" ],
      "stopOnEntry": false,
      "sourceMaps": true,
      "outDir": "${workspaceFolder}/out/src",
      "preLaunchTask": "npm"
    }
  ]
}
