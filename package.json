{
  "name": "markdown-spec-formatter",
  "displayName": "Markdown spec formatter",
  "version": "1.0.0",
  "description": "A markdown formatter for VSCode based on npm module: @quilicicf/markdown-formatter",
  "publisher": "quilicicf",
  "repository": "https://github.com/quilicicf/vscode-markdown-spec-formatter",
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./dist/extension",
  "license": "Apache-2.0",
  "contributes": {
    "languages": [
      {
        "id": "markdown",
        "aliases": [
          "MD"
        ],
        "extensions": [
          "md"
        ]
      }
    ]
  },
  "scripts": {
    "test": "tsc --noEmit",
    "build": "webpack --mode production",
    "build-dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "package": "npm run build && vsce package",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@quilicicf/markdown-formatter": "4.0.3",
    "snyk": "^1.683.0"
  },
  "devDependencies": {
    "@types/node": "16.6.1",
    "ts-loader": "9.2.5",
    "typescript": "4.3.5",
    "vsce": "1.96.1",
    "vscode": "1.1.37",
    "webpack": "5.50.0",
    "webpack-cli": "4.8.0"
  },
  "snyk": true
}
