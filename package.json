{
  "name": "markdown-spec-formatter",
  "displayName": "Markdown spec formatter",
  "version": "1.0.0",
  "description": "A markdown formatter for VSCode based on npm module: @quilicicf/markdown-formatter",
  "publisher": "quilicicf",
  "repository": "https://github.com/quilicicf/vscode-markdown-spec-formatter",
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./dist/extension",
  "license": "Apache-2.0",
  "contributes": {
    "languages": [
      {
        "id": "markdown",
        "aliases": [
          "MD"
        ],
        "extensions": [
          "md"
        ]
      }
    ]
  },
  "scripts": {
    "test": "tsc --noEmit",
    "build": "webpack --mode production",
    "build-dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "package": "npm run build && vsce package"
  },
  "dependencies": {
    "@quilicicf/markdown-formatter": "2.0.2"
  },
  "devDependencies": {
    "@types/node": "13.13.4",
    "ts-loader": "7.0.1",
    "typescript": "3.8.3",
    "vsce": "1.75.0",
    "vscode": "1.1.37",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  }
}
